{
	"manifest_version": 2,
	"name": "New-Tab",
	"version": "1.0",
	"background": {
		"scripts": ["main.js"]
	},
	"commands": {
		"new-tab-extension-command": {
			"suggested_key": {"default": "Alt+Shift+A"},
			"description": "new tab"
		}
	},
	"options_ui": {
		"page": "options.html"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"applications": {
		"gecko": {
			"id": "newtab@dansdomain.com"
		}
	}
}
